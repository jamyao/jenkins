pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                echo "Building the application"
                sh '''
                    set +x
                    for i in {1..10}
                    do
                        echo $i
                    done
                '''
            }
        }

        stage("test") {
            steps {
                echo "Testing the application"
            }
        }

        stage("deploy") {
            steps {
                echo "Deploy the application"
            }
        }

	    stage("canary") {
	        steps {
		        echo "Canary testing the application"
	        }
        }
    }
}
